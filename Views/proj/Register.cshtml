@model proj.RegisterUser

<a href="/">Login</a>
<p>Register</p>
<form asp-controller="proj" asp-action="Reg" method=post>

	<span asp-validation-for="Name"></span>
	<label asp-for="Name"></label>
		<input asp-for="Name"><br>

	<span asp-validation-for="Email"></span>
	<label asp-for="Email"></label>
		<input asp-for="Email"><br>

	<span asp-validation-for="Password"></span>
	<label asp-for="Password"></label>
		<input asp-for="Password"><br>

	<span asp-validation-for="PasswordConfirmation"></span>
	<label asp-for="PasswordConfirmation"></label>
		<input asp-for="PasswordConfirmation"><br>

	<button type="submit">Register</button>

</form>
